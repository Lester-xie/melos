    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "socket.io-client": "^4.5.0",
    "tone": "^14.7.77",
    "umi": "^3.5.23",
    "@babel/runtime": "^7.17.2",
    "event-emitter": "^0.3.4",
    "fade-curves": "^1.0.2",
    "fade-maker": "^1.0.3",
    "inline-worker": "^1.1.0",
    "lodash.assign": "^4.0.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.forown": "^4.0.0",
    "uuid": "^8.3.2",
